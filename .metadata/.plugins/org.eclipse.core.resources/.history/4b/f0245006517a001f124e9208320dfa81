package com.pm.jns;

import java.util.ArrayList;
import java.util.List;

import org.springframework.boot.CommandLineRunner;
import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.data.jpa.repository.config.EnableJpaRepositories;

import com.pm.jns.entity.BANK_MASTER_AGENCY;
import com.pm.jns.repository.BankMasterAgencyRepository;

import lombok.RequiredArgsConstructor;


@SpringBootApplication
@EnableJpaRepositories(basePackages = "com.pm.jns.repository")
@RequiredArgsConstructor
public class PmJnsDiyApplication implements CommandLineRunner{
	
	 static List<BANK_MASTER_AGENCY> fetch = new ArrayList<>();
	 
	 private final BankMasterAgencyRepository agencyRepository;
	
	
	public static void main(String[] args) {
		SpringApplication.run(PmJnsDiyApplication.class, args);
	}

	@Override
	public void run(String... args) throws Exception {
		// TODO Auto-generated method stub
		this.fetch=.getAllBankMasterDetails();
	}

}
